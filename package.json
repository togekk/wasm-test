{
  "scripts": {
    "asbuild:untouched": "asc assembly/index.ts -b build/untouched.wasm -t build/untouched.wat --sourceMap --validate",
    "asbuild:optimized": "asc assembly/index.ts -O3 -b build/optimized.wasm --validate --optimize --noDebug",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "parcel-bundler": "^1.9.6"
  }
}